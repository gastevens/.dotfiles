# ZSH Aliases

# Quick navigation
alias ..="cd .."
alias ...="cd ../.."
alias ....="cd ../../.."

# List files
alias ls="ls -G"
alias ll="ls -la"
alias la="ls -a"

# Git shortcuts
alias gs="git status"
alias gc="git commit"
alias gp="git push"
alias gl="git pull"

# Edit configuration files
alias zshrc="$EDITOR $DOTFILES/runcom/.zshrc"
alias dotfiles="cd $DOTFILES"

# Reload ZSH configuration
alias reload="source ~/.zshrc"

